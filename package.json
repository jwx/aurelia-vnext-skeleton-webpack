{
  "name": "skeleton-webpack",
  "version": "0.1.1",
  "scripts": {
    "watch": "webpack-dev-server --no-inline",
    "build": "rimraf dist && webpack --config webpack.config.js",
    "serve": "http-server -c-1 -p 9000 dist"
  },
  "dependencies": {
    "@aurelia/debug": "dev",
    "@aurelia/jit-html": "dev",
    "@aurelia/jit-html-browser": "dev",
    "@aurelia/jit": "dev",
    "@aurelia/kernel": "dev",
    "@aurelia/router": "dev",
    "@aurelia/runtime-html": "dev",
    "@aurelia/runtime": "dev"
  },
  "devDependencies": {
    "@types/node": "latest",
    "css-loader": "^2.1.1",
    "html-loader": "latest",
    "html-webpack-plugin": "latest",
    "http-server": "latest",
    "rimraf": "latest",
    "style-loader": "^0.23.1",
    "ts-loader": "latest",
    "typescript": "latest",
    "webpack": "latest",
    "webpack-cli": "latest",
    "webpack-dev-server": "latest"
  }
}
